public class Credits {

    private List<Credit> info;
    private boolean BasicInformationState;
    private JudmentLoanState JudgmentLoanStatus;//基本信息是否填写

    public JudmentLoanState getJudgmentLoanStatus() {
        return JudgmentLoanStatus;
    }

    public void setJudgmentLoanStatus(JudmentLoanState judgmentLoanStatus) {
        JudgmentLoanStatus = judgmentLoanStatus;
    }

    public List<Credit> getInfo() {
        return info;
    }

    public void setInfo(List<Credit> info) {
        this.info = info;
    }

    public boolean isBasicInformationState() {
        return BasicInformationState;
    }

    public void setBasicInformationState(boolean basicInformationState) {
        BasicInformationState = basicInformationState;
    }
}
