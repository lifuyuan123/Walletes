<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/app_space_bg"
    android:orientation="vertical">


    <com.wallet.bo.wallets.ui.weiget.EaseTitleBar
        android:id="@+id/ease_titlebar"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:clipToPadding="true"
        android:fitsSystemWindows="true"
        app:titleBarShowLine="true"
        app:titleBarBackground="@color/white"
        app:titleBarLeftImage="@drawable/im_close"
        app:titleBarTitleColor="@color/text_gray"
        app:titleBarTitle="设置交易密码">

    </com.wallet.bo.wallets.ui.weiget.EaseTitleBar>

    <com.wallet.bo.wallets.ui.weiget.BounceScrollView
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:scrollbars="none">


        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:padding="10dip"
            android:orientation="vertical">


            <TextView
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:padding="15dip"
                android:gravity="center"
                android:layout_marginTop="20dip"
                android:text="请设置6位数字交易密码"
                android:textColor="@android:color/white"
                android:textSize="16sp" />

            <com.wallet.bo.wallets.ui.weiget.SecurityPasswordEditText
                android:id="@+id/se_pw"
                android:layout_width="match_parent"
                android:layout_height="wrap_content">

            </com.wallet.bo.wallets.ui.weiget.SecurityPasswordEditText>

        </LinearLayout>

    </com.wallet.bo.wallets.ui.weiget.BounceScrollView>


</LinearLayout>