<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/app_space_bg"
    android:orientation="vertical">


    <com.wallet.bo.wallets.ui.weiget.EaseTitleBar
        android:id="@+id/ease_titlebar"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:clipToPadding="true"
        android:fitsSystemWindows="true"
      app:titleBarBackground="@color/app_theme"
        app:titleBarLeftImage="@drawable/im_close"

        app:titleBarShowLine="true"
        app:titleBarTitle="个人设置">

    </com.wallet.bo.wallets.ui.weiget.EaseTitleBar>

    <com.wallet.bo.wallets.ui.weiget.BounceScrollView
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:layout_marginBottom="15dip"
        android:scrollbars="none">


        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:orientation="vertical">


            <TextView
                android:id="@+id/tv_zl"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:padding="15dip"
                android:drawablePadding="10dip"
                android:background="@color/white"
                android:drawableRight="@drawable/im_spinner_right"
                android:gravity="center_vertical"
                android:text="个人资料"
                android:textColor="@color/text_gray"
                android:textSize="16sp" />

            <include layout="@layout/item_horizontal_gray" />


            <TextView
                android:id="@+id/tv_aq"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:padding="15dip"
                android:drawablePadding="10dip"
                android:background="@color/white"
                android:drawableRight="@drawable/im_spinner_right"
                android:gravity="center_vertical"
                android:text="账户安全"
                android:textColor="@color/text_gray"
                android:textSize="16sp" />

            <include layout="@layout/item_horizontal_gray" />

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:background="@color/white"
                android:orientation="horizontal">


                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:padding="15dip"
                    android:gravity="center_vertical"
                    android:text="清除缓存"
                    android:textColor="@color/text_gray"
                    android:textSize="16sp" />


                <TextView
                    android:id="@+id/tv_chache"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:padding="15dip"
                    android:drawablePadding="10dip"
                    android:drawableRight="@drawable/im_spinner_right"
                    android:gravity="right|center_vertical"
                    android:text="0M"
                    android:textColor="@color/text_gray"
                    android:textSize="16sp" />
            </LinearLayout>



            <include layout="@layout/item_horizontal_gray" />


            <android.support.v7.widget.SwitchCompat
                android:id="@+id/sc_pushEnable"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:padding="15dip"
                android:text="消息推送"
                android:background="@color/white"
                android:theme="@style/SwitchCompat"
                android:textSize="16sp"
                android:textColor="@color/text_gray" />


            <include layout="@layout/item_horizontal_gray" />

            <Button
                android:id="@+id/tv_exit"
               style="@style/bt_goto"
                android:text="退出" />

            <include layout="@layout/item_horizontal_gray" />

        </LinearLayout>


    </com.wallet.bo.wallets.ui.weiget.BounceScrollView>


</LinearLayout>